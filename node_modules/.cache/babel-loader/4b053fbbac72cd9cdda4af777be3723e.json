OAAA,CAAA,MAAA,GAAA,UAAA,CAAA,MAAA;AACA,SAFA,EAEA,GAFA,CAAA;AAGA,OAJA;AAfA;;AAEA,SAAA,IAAA,EAAA,IAAA,KAAA,OAAA,EAAA;AAAA,UACA,IADA;AAAA,UAEA,CAFA;;AAAA;AAkBA;AACA,GAvCA;AAwCA,EAAA,KAAA,EAAA,EAxCA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,WAAA;AACA,KAHA;AAIA,IAAA,WAJA,yBAIA;AAAA;;AACA,UAAA,QAAA,GAAA,CAAA,UAAA,EAAA,MAAA,EAAA,MAAA,CAAA;AACA,UAAA,GAAA,GAAA,CAAA;;AAFA;AAIA,YAAA,EAAA,GAAA,QAAA,IAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,mBAAA,EAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,EAAA,IAAA,QAAA,CAAA,IAAA;AAEA,UAAA,GAAA,IAAA,CAAA;;AACA,cAAA,GAAA,IAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA,GAAA,KAAA;AACA;AACA,SAPA;AALA;;AAIA,4BAAA,QAAA,eAAA;AAAA;AASA;AACA;AAlBA;AA3CA,CAAA","sourcesContent":["<template>\n<section style=\"padding: 4rem 2rem\">\n<h5><a href=\"/\">\n  <i class=\"fa fa-home\"></i> Home</a>&nbsp;<i class=\"fas fa-chevron-right\"></i>&nbsp;\n Antigenic Epitopes</h5>\n<br>\n\n<div v-if=\"isLoadingResult\" style=\"text-align: center\">\n    <br />\n    <hollow-dots-spinner\n        :animation-duration=\"1000\"\n        :dot-size=\"20\"\n        :dots-num=\"3\"\n        color=\"#00d961\"\n        style=\"margin-left: auto; margin-right: auto;\"\n    />\n    <br />\n    <span class=\"badge badge-success\">Loading Data...</span>\n</div>\n<div v-if=\"!isLoadingResult\">\n<ul class=\"nav nav-tabs\" id=\"mainTab\" role=\"tablist\">\n  <li class=\"nav-item\">\n    <a class=\"nav-link active\" id=\"sarscov2-tab\" data-toggle=\"tab\" href=\"#home\" role=\"tab\" aria-controls=\"home\" aria-selected=\"true\">SARS-COV-2 Epitopes</a>\n  </li>\n  <li class=\"nav-item\">\n    <a class=\"nav-link\" id=\"sars-tab\" data-toggle=\"tab\" href=\"#map\" role=\"tab\" aria-controls=\"map\" aria-selected=\"false\">SARS-COV Epitopes</a>\n  </li>\n  <li class=\"nav-item\">\n    <a class=\"nav-link\" id=\"mers-tab\" data-toggle=\"tab\" href=\"#hist\" role=\"tab\" aria-controls=\"hist\" aria-selected=\"false\">MERS-COV Epitopes</a>\n  </li>\n</ul>\n\n<div class=\"tab-content\" id=\"mainTabContent\">\n  <div class=\"tab-pane fade show active\" id=\"home\" role=\"tabpanel\" aria-labelledby=\"home-tab\">\n    <br>\n    <section>\n    <table id=\"sarscov2_ep\" class=\"display nowrap table\" style=\"width:100%\">\n    </table>\n    <br>\n    <br>\n   </section>\n   &nbsp;\n\n  </div>\n\n\n  <div class=\"tab-pane fade\" id=\"map\" role=\"tabpanel\" aria-labelledby=\"map-tab\" style=\"width:100%\">\n    <br>\n      <section>\n      <table id=\"sars_ep\" class=\"display nowrap table\" style=\"width:100%\">\n      </table>\n      <br>\n      <br>\n    </section>\n    &nbsp;\n  </div>\n\n\n  <div class=\"tab-pane fade\" id=\"hist\" role=\"tabpanel\" aria-labelledby=\"hist-tab\" style=\"width:100%\">\n      <br>\n      <section>\n        <table id=\"mers_ep\" class=\"display nowrap table\" style=\"width:100%\">\n        </table>\n        <br>\n        <br>\n      </section>\n    &nbsp;\n  </div>\n</div>\n\n</div>\n\n</section>\n\n</template>\n\n\n<script>\nimport axios from 'axios'\n\nimport { HollowDotsSpinner } from 'epic-spinners'\n\nexport default {\n  name: \"epitopes\",\n  components: {\n    HollowDotsSpinner\n  },\n  data () {\n    return {\n      isLoadingResult: true,\n      alldata: {\n        \"sarscov2\": null,\n        \"sars\": null,\n        \"mers\": null,\n        }\n    }\n  },\n  created () {\n    this.fetchData();\n  },\n  updated: function () {\n\n    for(var ep in this.alldata) {\n      var cols = [];\n      for(var k in this.alldata[ep][0]) {\n        cols.push({\"data\":k, \"title\": k.replace(/_/g, \" \")})\n      }\n\n      let epTable = $('#' + ep + \"_ep\").DataTable({\n        columns: cols,\n        data: this.alldata[ep],\n        order: [[ 2, \"asc\" ]],\n        responsive: true,\n      });\n\n      $('#' + ep + '-tab').on('click', e=> {\n        setTimeout(()=>{\n          epTable.columns.adjust().responsive.recalc();\n        }, 500);\n      });\n    }\n  },\n  watch: {\n\n  },\n  methods: {\n    fetchData(){\n      this.getEpitopes();\n    },\n    getEpitopes() {\n      var epitopes = [\"sarscov2\", \"sars\", \"mers\"];\n      var tot = 0;\n      \n      for(let ep of epitopes){\n        axios.get(\"/api/epitopes/\" + ep).then(response=>{\n          this.alldata[ep] = response.data;\n\n          tot += 1;\n          if(tot == 3){\n            this.isLoadingResult = false;\n          }\n        });\n      }\n    },\n  }\n};\n</script>\n\n<style>\n.crit {\n    font-size: 75%;\n}\n</style>\n\n"],"sourceRoot":"src/views"}]}